  "scripts": {
    "start": "cross-env DESKTOP=true electron-forge start",
    "start:web": "cross-env DESKTOP=false webpack serve --node-env=development --config webpack.config.ts",
    "build": "yarn build:prod",
    "build:dev": "cross-env DESKTOP=false webpack --node-env=development --config webpack.config.ts",
    "build:prod": "cross-env DESKTOP=false webpack --node-env=production --config webpack.config.ts",
    "package:local-server": "cross-env LOCAL_SERVER=true DESKTOP=true electron-forge package",
    "package:cloud-server": "cross-env LOCAL_SERVER=false DESKTOP=true electron-forge package",
    "make:local-server": "cross-env LOCAL_SERVER=true DESKTOP=true electron-forge make",
    "make:cloud-server": "cross-env LOCAL_SERVER=false DESKTOP=true electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "postinstall": "husky",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "test:mutation": "stryker run",
    "generate-api": "yarn openapi-generator-cli generate && npx ts-node src/shared/api/helpers/addTsNocheckPostGeneration.ts"
  },
